// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Property {
  id           Int     @id @default(autoincrement())
  propertyName String
  ownerName    String
  localityId   Int
  locality     String
  city         String?
  postalCode   Int?
  published    Boolean @default(false)
}
